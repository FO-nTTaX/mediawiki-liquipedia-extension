{
	"name": "LiquiGoals",
	"version": "1.0",
	"author": [
		"Jérémy Touati"
	],
	"url": "http://wiki.teamliquid.net/",
	"descriptionmsg": "liquigoals-desc",
	"license-name": "MIT",
	"type": "pleesher",
	"requires": {
		"MediaWiki": ">= 1.25.0",
		"Pleesher": ">= 0.9"
	},
	
	"callback": "LiquiGoals::initialize",
	
	"MessagesDirs": {
		"LiquiGoals": [
			"i18n"
		]
	},
	
	"AutoloadClasses": {
		"LiquiGoals": "LiquiGoals.php",
		"LiquiGoals_PleesherImplementation": "PleesherImplementation.php",
		"LiquiGoals_QueryHelper": "QueryHelper.php",
		"LiquiGoals_ProfessionListPage": "pages/ProfessionListPage.php"
	},
	"Hooks": {
		"ParserFirstCallInit": [
			"LiquiGoals::initializeParser"
		],
		"BeforePageDisplay": [
			"LiquiGoals::beforePageDisplay"
		],
		"ExtensionTypes": [
			"LiquiGoals::extensionTypes"
		]
	},
	"SpecialPages": {
		"Professions": "LiquiGoals_ProfessionListPage"
	},
	"ResourceModules": {
		"ext.liquigoals": {
			"styles": [
				"styles.less"
			],
			"position": "top"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "LiquiGoals/resources"
	},
	"manifest_version": 1
}